generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid()) @db.Uuid
  name       String
  email      String    @unique
  created_at DateTime  @default(now())
  updated_at DateTime?

  @@map("users")
}
